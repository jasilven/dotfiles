[push]
	default = matching
[core]
	excludesfile = ~/.config/git/ignore

[color "status"]
  added = green
  changed = yellow 
  untracked = magenta bold

[color "branch"]
  remote = yellow
[alias]
	logline = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[pull]
	rebase = false
