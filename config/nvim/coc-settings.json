{
  "explorer.floating.position": "center-top",
  "explorer.keyMappings": {
    "i": false, // cancel default mapkey
    "gk": "expandablePrev",
    "gj": "expandableNext",
    "*": "toggleSelection",
    "<tab>": "actionMenu",
    "h": "collapse",
    "l": [
      "expandable?",
      "expand",
      "open"
    ],
    "J": [
      "toggleSelection",
      "nodeNext"
    ],
    "K": [
      "toggleSelection",
      "nodePrev"
    ],
    "R": [
      "expandable?",
      "cd",
      "false"
    ],
    "gl": [
      "expandable?",
      "cd"
    ],
    "U": "gotoParent",
    "gh": "gotoParent",
    "<2-LeftMouse>": [
      "expandable?",
      [
        "expanded?",
        "collapse",
        "expand"
      ],
      "open"
    ],
    "o": [
      "expanded?",
      "collapse",
      "expand"
    ],
    "<cr>": [
      "expandable?",
      [
        "expanded?",
        "collapse",
        "expand"
      ],
      "open"
    ],
    "e": "open",
    "s": "open:split",
    "S": "open:split:plain",
    "v": "open:vsplit",
    "t": "open:tab",
    "<bs>": "collapseRecursive",
    "gp": "preview:labeling",
    "y": "copyFilepath",
    "Y": "copyFilename",
    "c": "copyFile",
    "x": "cutFile",
    "p": "pasteFile",
    "d": "delete",
    "D": "deleteForever",
    "a": "addFile",
    "A": "addDirectory",
    "r": "rename",
    ".": "toggleHidden",
    "gr": "refresh",
    "?": "help",
    "q": "quit",
    "<esc>": "esc",
    "X": "systemExecute",
    "gd": "listDrive",
    "f": "search",
    "F": "searchRecursive",
    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",
    "[[": "sourcePrev",
    "]]": "sourceNext",
    "[d": "diagnosticPrev",
    "]d": "diagnosticNext",
    "[c": "gitPrev",
    "]c": "gitNext",
    "<<": "gitStage",
    ">>": "gitUnstage"
  },
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "highlight.document.enable": true,
  "rust.clippy_preference": "on",
  "coc-actions.showActionKind": true,
  "rust-analyzer.diagnostics.enable": true,
  "json.enable": true,
  "codeLens.enable": true,
  "codeLens.separator": " ",
  "json.format.enable": true,
  "coc.preferences.formatOnType": true,
  "coc.source.buffer.ignoreGitignore": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "rust",
    "clojure",
    "go",
    "json"
  ],
  "coc.preferences.snippets.enable": true,
  "diagnostic.enableSign": true,
  "diagnostic.joinMessageLines": true,
  "diagnostic.messageTarget": "float",
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualText": true,
  "diagnostic.infoSign": "i",
  "diagnostic.errorSign": "!",
  "diagnostic.warningSign": "?",
  "diagnostic.highlightOffset": 1000,
  "diagnostic.hintSign": "i",
  "diagnostic.refreshOnInsertMode": true,
  "suggest.autoTrigger": "always",
  "suggest.snippetIndicator": "~",
  "suggest.labelMaxLength": 60,
  "suggest.maxPreviewWidth": 60,
  "suggest.detailMaxLength": 60,
  "suggest.enablePreview": true,
  "suggest.disableKind": false,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "languageserver": {
    "clojure-lsp": {
      "command": "bash",
      "args": [
        "-c",
        "clojure-lsp"
      ],
      "filetypes": [
        "clojure"
      ],
      "rootPatterns": [
        "project.clj",
        "deps.edn"
      ],
      "additionalSchemes": [
        "jar",
        "zipfile"
      ],
      "trace.server": "verbose",
      "initializationOptions": {}
    },
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  }
}
